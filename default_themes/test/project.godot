;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Resource",
"class": "RetroHubGameData",
"language": "GDScript",
"path": "res://addons/retrohub_theme_helper/classes/GameData.gd"
}, {
"base": "Resource",
"class": "RetroHubGameMediaData",
"language": "GDScript",
"path": "res://addons/retrohub_theme_helper/classes/GameMediaData.gd"
}, {
"base": "Resource",
"class": "RetroHubSystemData",
"language": "GDScript",
"path": "res://addons/retrohub_theme_helper/classes/SystemData.gd"
} ]
_global_script_class_icons={
"RetroHubGameData": "",
"RetroHubGameMediaData": "",
"RetroHubSystemData": ""
}

[application]

config/name="Test Theme"
run/main_scene="res://Theme.tscn"
config/icon="res://icon.png"

[autoload]

RetroHub="*res://addons/retrohub_theme_helper/RetroHub.gd"

[editor_plugins]

enabled=PoolStringArray( "res://addons/retrohub_theme_helper/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
